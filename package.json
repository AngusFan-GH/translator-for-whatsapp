{
  "name": "translate-for-whatsapp",
  "version": "1.3.2",
  "description": "A translator for WhatsApp.",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "install-popup": "cd ./src/popup/ && npm install",
    "install-all": "npm install && npm run install-popup",
    "start-popup": "cd ./src/popup/ && npm run serve",
    "build-popup": "cd ./src/popup/ && npm run build",
    "start": "concurrently \"webpack --watch --config webpack.dev.js\" \"npm run start-popup\"",
    "test": "npm run build-popup && webpack --config webpack.qa.js",
    "build": "npm run build-popup && webpack --config webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AngusFan-GH/translator-for-whatsapp.git"
  },
  "author": "Angus",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AngusFan-GH/translator-for-whatsapp/issues"
  },
  "homepage": "https://github.com/AngusFan-GH/translator-for-whatsapp#readme",
  "keywords": [
    "chrome-extension",
    "WhatsApp"
  ],
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel": "^6.23.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.1.0",
    "core-js": "^3.6.4",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "file-loader": "^5.0.2",
    "filemanager-webpack-plugin": "^4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "thread-loader": "^2.1.3",
    "uglify-js": "^3.13.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^5.7.3",
    "workbox-webpack-plugin": "^5.0.0"
  },
  "dependencies": {
    "@eastdesire/jscolor": "^2.4.5",
    "axios": "^0.21.1",
    "copy-webpack-plugin": "^6.2.1",
    "jquery": "^3.4.1",
    "mime-types": "^2.1.30",
    "rxjs": "^6.6.6"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "browser": true,
      "webextensions": true
    }
  },
  "sideEffects": [
    "*.css"
  ]
}